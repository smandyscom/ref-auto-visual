Imports Automation

Public Class FormConveyorTest
    Dim __pecvdAssembly As IOXLoaderAssembly = New IOXLoaderAssembly
    Private Sub FormConveyorTest_Load(sender As Object, e As EventArgs) Handles MyBase.Load


        standardInitialRoutine()

        With __pecvdAssembly
            .CentralAlarmObject = New Automation.alarmManager
            .CentralMessenger = New Automation.messageHandler
            .PauseBlock = New Automation.interceptObject
        End With

        __pecvdAssembly.initialize()


        Me.Panel1.Controls.Add(__pecvdAssembly.raisingGUI())

        Dim __error As Integer
        For Each __i As Integer In {motorAddress.MSE1A, motorAddress.MSE1C, motorAddress.MSE1E,
                                                    motorAddress.MSF1A, motorAddress.MSF1C, motorAddress.MSF1E,
                                                    motorAddress.MSF1B, motorAddress.MSE1D,
                                                    motorAddress.MSG1C, motorAddress.MSG1B, motorAddress.MSG1A}
            AMaxM4_ServoOn(__i, IS_ON)
            AMaxM4_ErrorStatus(__i, __error)
        Next

        alarmContentSensor.InputsEnumType = GetType(inputAddress)
        alarmContentSensor.QueryInterface = Nothing

        __pecvdAssembly.IsEnabled = True
        __pecvdAssembly.start()

        'UserControl11.__conveyorAssembly = __pecvdAssembly.conveyorsAssembly
        'UserControl11.Timer1.Enabled = True
    End Sub

    Private Sub Button1_Click(sender As System.Object, e As System.EventArgs) Handles Button1.Click
        Dim __formSetting As formSetting = New formSetting
        __formSetting.ShowDialog()
    End Sub

