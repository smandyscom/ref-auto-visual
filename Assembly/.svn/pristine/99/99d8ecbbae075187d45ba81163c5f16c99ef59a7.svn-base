    Sub lbl_BackColor(bln As Boolean, lbl As Label)
        If bln = True AndAlso lbl.BackColor <> Color.LimeGreen Then
            lbl.BackColor = Color.LimeGreen
        ElseIf bln = False AndAlso lbl.BackColor <> SystemColors.Control Then
            lbl.BackColor = SystemColors.Control
        End If
    End Sub

