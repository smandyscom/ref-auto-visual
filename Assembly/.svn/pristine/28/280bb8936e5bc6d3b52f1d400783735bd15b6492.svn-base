<?xml version="1.0" encoding="utf-8"?>
<mainIOHardware xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <PhysicalHardwareList>
    <subHardwareNode>
      <PhysicalHardware xsi:type="virtualModule">
        <MemorySize>256</MemorySize>
      </PhysicalHardware>
    </subHardwareNode>
    <subHardwareNode>
      <PhysicalHardware xsi:type="dmtModbusInterface">
        <MasterList>
          <dmtModbusMaster>
            <ConnectionInterface>SOCKET</ConnectionInterface>
            <CommunicationPort>1</CommunicationPort>
            <Baud>B115200</Baud>
            <DataLength>LEN_8</DataLength>
            <Parity>EVEN</Parity>
            <StopBits>BITS_1</StopBits>
            <Mode>RTU</Mode>
            <ConnectionIp>192.168.1.5</ConnectionIp>
            <SlaveList>
              <dmtModbusSlave>
                <SlaveType>DVP</SlaveType>
                <SlaveAddress>1</SlaveAddress>
                <XRange>32</XRange>
                <YRange>32</YRange>
                <DReadRange>8</DReadRange>
                <DWriteRange>8</DWriteRange>
                <SlaveName>PLC</SlaveName>
              </dmtModbusSlave>
            </SlaveList>
            <PollingInterval>10</PollingInterval>
            <ReconnectInterval>5000</ReconnectInterval>
          </dmtModbusMaster>
        </MasterList>
      </PhysicalHardware>
    </subHardwareNode>
  </PhysicalHardwareList>
</mainIOHardware>