Imports MathNet.Numerics.LinearAlgebra

''' <summary>
''' The error matrix
''' </summary>
''' <remarks></remarks>
Public Class errorHtm
    Inherits htmEdgeElementary

    ''' <summary>
    ''' The 6x1 matrix
    ''' </summary>
    ''' <value></value>
    ''' <returns></returns>
    ''' <remarks></remarks>
    Property ErrorVector As Vector(Of Double)
        Get

        End Get
        Set(value As Vector(Of Double))

        End Set
    End Property

    Sub New(__from As [Enum],
            __to As [Enum])
        MyBase.New(__from, __to)
    End Sub

    ''' <summary>
    ''' Error is defined by nominal-real
    ''' </summary>
    ''' <param name="nominalPostion"></param>
    ''' <returns></returns>
    ''' <remarks></remarks>
    Shared Function genErrorGain(nominalPostion As PositionVector) As Matrix(Of Double)
        With nominalPostion
            Return CreateMatrix.Dense(Of Double)(3, 6, {0, .Z, .Y, -1, 0, 0,
                                                        -.Z, 0, -.X, 0, -1, 0,
                                                        -.Y, .X, 0, 0, 0, -1})
        End With
    End Function


End Class

